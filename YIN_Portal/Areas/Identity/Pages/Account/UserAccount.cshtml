@page
@using YIN_Portal.Areas.Identity.Pages;
@model LoginModel

@{
    ViewData["Title"] = "User Account";
}

@*<h1>@ViewData["Title"]</h1>*@

<!DOCTYPE html>
<html>

<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no" />
    <title>User Account</title>
    <link rel="shortcut icon" href="/plugins/images/pak-logo-tr2.png" />
    <link rel="apple-touch-icon" sizes="512x512" href="/plugins/images/pak-logo-tr2.png" />

    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.6.2/dist/css/bootstrap.min.css">
    <script src="https://cdn.jsdelivr.net/npm/jquery@3.5.1/dist/jquery.slim.min.js" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.1/dist/umd/popper.min.js" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.6.2/dist/js/bootstrap.min.js" crossorigin="anonymous"></script>

    <!-- <link href="~/css/bootstrap/bootstrap.min.css" rel="stylesheet" />
    <link href="~/css/app.css" rel="stylesheet" /> -->
    <!-- Font Awesome -->
    <link rel="stylesheet" href="/plugins/fontawesome-free/css/all.min.css">
    <!-- CSS Files -->
    <link href="/wizard/css/bootstrap.min.css" rel="stylesheet" />
    <link href="/wizard/css/gsdk-bootstrap-wizard.css" rel="stylesheet" />



    <link href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400i,700" rel="stylesheet">

    <style>
        
        .imagesB {
            /*background-image: url("/plugins/images/background.png");*/
            background-image: url("/img/Rectangle.png");
            background-color: #ffffff;
            background-repeat: no-repeat;
            background-size: cover;
            height: 100vh;
        }

        
    </style>
</head>

<body class="container-fluid bg-white vh-100 overflow-hidden imagesB p-0">
    <div>
        <div>
            <h3 class="text-center">Create New Account</h3>
            <label>looking for Advertismen?<a href="#">Apply as Influencer</a></label>
        </div>
    </div>
    <div class="row">
       
        <div class="col-sm-8 col-sm-offset-2">

            <!--      Wizard container        -->
            <div class="wizard-container">
                <div class="card wizard-card" data-color="azzure" id="wizard">
                    <form action="" method="" novalidate="novalidate">
                        <!--        You can switch ' data-color="azzure" '  with one of the next bright colors: "blue", "green", "orange", "red"          -->

                      
                        <div class="wizard-navigation">
                            <ul class="nav nav-pills">
                                <li class="active" style="width: 50%;"><a href="#details" data-toggle="tab" aria-expanded="true">Sign up Basic Information</a></li>
                                <li class="" style="width: 50%;"><a href="#captain" data-toggle="tab" aria-expanded="false">Business Information</a></li>                                
                            </ul>
                            <div class="moving-tab" style="width: 205.667px; transform: translate3d(0px, 0px, 0px); transition: all 0.3s ease-out 0s;">Details</div>
                        </div>
                        <div class="tab-content">
                            <div class="tab-pane active" id="details">
                                <div class="row">
                                    <div class="col-sm-12">
                                        <button class="btn  btn-block col-sm-10 col-sm-offset-1 my-4" style="background-color: #ECF2FC;  color:black;"
                                                type="submit">
                                            <i class="me-2 fab fa-apple">&nbsp;</i>  Continue with Apple
                                        </button>
                                    </div>
                                    </div>
                                <div class="row">
                                    <div class="col-sm-12">
                                        <button class="btn  btn-block  mb-2 col-sm-10 col-sm-offset-1" style="background-color: #4285F4; color:white"
                                                type="submit">
                                            <i class="fab fa-google me-2"></i> Continue with Google
                                        </button>
                                    </div>
                                </div>
                                    <div class="row">
                                    <div class="col-sm-5 col-sm-offset-1">
                                        <div class="form-group">
                                            <label>What city is your boat in?</label>
                                            <input type="text" class="form-control" id="exampleInputEmail1" placeholder="Where is your boat located?">
                                        </div>
                                    </div>
                                    <div class="col-sm-5">
                                        <div class="form-group">
                                            <label>Boat Type</label>
                                            <select class="form-control">
                                                <option disabled="" selected="">- boat type -</option>
                                                <option>Power</option>
                                                <option>Sail</option>
                                                <option>Paddle</option>
                                            </select>
                                        </div>
                                    </div>
                                    <div class="col-sm-5 col-sm-offset-1">
                                        <div class="form-group">
                                            <label>Year Manufacture</label>
                                            <select class="form-control">
                                                <option disabled="" selected="">- year -</option>
                                                <option>2008</option>
                                                <option>2009</option>
                                                <option>2010</option>
                                                <option>2011</option>
                                                <option>2012</option>
                                                <option>2013</option>
                                                <option>2014</option>
                                            </select>
                                        </div>
                                    </div>
                                    <div class="col-sm-5">
                                        <div class="form-group">
                                            <label>Daily Price</label>
                                            <div class="input-group">
                                                <input type="text" class="form-control">
                                                <span class="input-group-addon">$</span>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="tab-pane" id="captain">
                                <h4 class="info-text">Do you include a captain? </h4>
                                <div class="row">
                                    <div class="col-sm-10 col-sm-offset-1">
                                        <div class="col-sm-4 col-sm-offset-2">
                                            <div class="choice" data-toggle="wizard-radio" rel="tooltip" title="" data-original-title="Renters you approve will be able to take this boat">
                                                <input type="radio" name="job" value="Design">
                                                <div class="icon">
                                                    <i class="fa fa-life-ring"></i>
                                                </div>
                                                <h6>No Captain</h6>
                                            </div>
                                        </div>
                                        <div class="col-sm-4">
                                            <div class="choice" data-toggle="wizard-radio" rel="tooltip" title="" data-original-title="Select this option if you or a certified captain will be included.">
                                                <input type="radio" name="job" value="Code">
                                                <div class="icon">
                                                    <i class="fa fa-male"></i>
                                                </div>
                                                <h6>Includes Captain</h6>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        
                        </div>
                        <div class="wizard-footer">
                            <div class="pull-right">
                                <input type="button" class="btn btn-next btn-fill btn-info btn-wd btn-sm" name="next" value="Next" style="">
                                <input type="button" class="btn btn-finish btn-fill btn-info btn-wd btn-sm" name="finish" value="Finish" style="display: none;">
                            </div>
                            <div class="pull-left">
                                <input type="button" class="btn btn-previous btn-fill btn-default btn-wd btn-sm disabled" name="previous" value="Previous">
                            </div>
                            <div class="clearfix"></div>
                        </div>
                    </form>
                </div>
            </div> <!-- wizard container -->
        </div>
    </div>



</body>

<!--   Core JS Files   -->
<script src="/wizard/js/jquery-2.2.4.min.js" type="text/javascript"></script>
<script src="/wizard/js/bootstrap.min.js" type="text/javascript"></script>
<script src="/wizard/js/jquery.bootstrap.wizard.js" type="text/javascript"></script>

<!--  Plugin for the Wizard -->
<script src="/wizard/js/gsdk-bootstrap-wizard.js"></script>

<!--  More information about jquery.validate here: http://jqueryvalidation.org/	 -->
<script src="/wizard/js/jquery.validate.min.js"></script>
</html>



